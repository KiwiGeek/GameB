{
	"version": "2.0.0",
	"tasks": [
		{
			"label": "Game build",
			"type": "shell",
			"command": "${workspaceFolder}\\build.bat",
			"group": {
				"kind": "build",
				"isDefault": true
			},
			"presentation": {
				"reveal": "always",
				"showReuseMessage": false,
				"panel": "dedicated"
			},
			"options": {
				"cwd": "${workspaceFolder}"
			},
			"problemMatcher": "$msCompile"
		}
	]
}